      <property name="script">var dbConn = DatabaseConnectionFactory.createDatabaseConnection(globalMap.get(&quot;jdbcDriver&quot;), globalMap.get(&quot;odkJdbcDatabaseUrl&quot;), globalMap.get(&quot;dbUser&quot;), globalMap.get(&quot;dbPass&quot;));
var result = dbConn.executeCachedQuery(&quot;SELECT _URI, _CREATION_DATE, SECTION6_LAST_NAME, SECTION6_FIRST_NAME, SECTION5_FR_SEC, SUPERVISOR_STATUS, SECTION6_GENDER, SECTION5_FR_VILL, SECTION5_FR_PN, EVER_REG, HOUSEHOLD_NAME, VALIDATION_FAILED, VISIT_DATE, SECTION6_MOTHER_ID, TEMP_ID, RELATIONSHIP_TO_GROUP_HEAD, SECTION5_FR_HN, MFROM_HH_N, INDIVIDUAL_ID, FROM_OTHER, MFROM_HN, MIGRATION_REASON, FROM_FIELD, SECTION5_FR_ON, HOUSE_ID, OLD_NAME, MFROM_VILL, SECTION5_FR_HH_N, HOUSE_NAME, DATE_MI, SECTION6_DOB, SECTION6_MIN_PERSON_EDUCATION, DERIVED_FROM_URI, MFROM_SEC, SECTION6_FATHER_ID, FIELD_WORKER_ID, HOUSEHOLD_ID, MFROM_PN, PROCESSED_BY_MIRTH, MIGRATION_REASON_OTHER FROM &quot; +  globalMap.get(&quot;migrationInTableName&quot;) + &quot; WHERE processed_by_mirth is null AND supervisor_status is null&quot;);
dbConn.close();

return result;</property>
