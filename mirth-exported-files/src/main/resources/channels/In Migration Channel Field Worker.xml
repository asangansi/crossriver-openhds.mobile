var auditXml = constructAuditXml(instanceXml, null, &quot;In Migration&quot;, origXml[&apos;field_worker_id&apos;].toString(), origXml[&apos;_uri&apos;].toString(), globalMap.get(&quot;migrationInFormName&quot;), origXml[&apos;derived_from_uri&apos;].toString());

router.routeMessage(&quot;Form Submission Channel&quot;, auditXml);
return;</property>
