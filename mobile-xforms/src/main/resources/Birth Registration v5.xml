			<bind nodeset="/data/firstLiveBirth" type="select1" required="true()" />
			<bind nodeset="/data/numLiveBirths" type="int"
				relevant="/data/firstLiveBirth='2'" required="true()" />
			<bind nodeset="/data/totalChildrenBorn" type="int" required="true()" />
			<bind nodeset="/data/numLiveBirthsFromRecentPreg" type="int"
				required="true()" constraint=". &lt;= ../totalChildrenBorn"
				jr:constraintMsg="Number of live births cannot be greater than total number of children born for this pregnancy" />
			<bind nodeset="/data/children/child1Id" type="string"
