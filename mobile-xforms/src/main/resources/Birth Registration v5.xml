			<bind nodeset="/data/children/secondChild" type="select1" required="true()" />
			<bind nodeset="/data/children/child2Id" type="string"
				constraint="regex(., '^[A-Za-z]{9}[0-9]{7}$')" required="true()"
				relevant="/data/children/secondChild='1'" />
			<bind nodeset="/data/children/secondChildFirstName" type="string"
				required="true()" relevant="/data/children/secondChild='1'" />
			<bind nodeset="/data/children/secondChildLastName" type="string"
				required="true()" relevant="/data/children/secondChild='1'" />
			<bind nodeset="/data/children/secondChildSex" type="select1"
