			<bind nodeset="/data/houseName" type="string" required="true()" />
			<bind nodeset="/data/householdId" type="string" required="true()"
				constraint="regex(., '^[A-Za-z]{9}[0-9]{5}$')" jr:constraintMsg="Household Id is not the right format" />
			<bind nodeset="/data/householdName" type="string" required="true()" />
			<bind nodeset="/data/childDob" type="date" required="true()"
				constraint=". &lt; /data/visitDate" jr:constraintMsg="The date of birth cannot be after the interview date" />
			<bind nodeset="/data/childFatherId" type="string" required="true()"
				constraint="regex(., '^([A-Za-z]{9}[0-9]{7}$|^[Uu][Nn][Kk])$')"
				jr:constraintMsg="Permanent Id is not the right format" />
			<bind nodeset="/data/childFatherFirstName" type="string"
				relevant="not(regex(../fatherId, '^[Uu][Nn][Kk]$'))" required="true()" />
			<bind nodeset="/data/childFatherLastName" type="string"
				relevant="not(regex(../fatherId, '^[Uu][Nn][Kk]$'))" required="true()" />
			<bind nodeset="/data/birthPlace" type="select1" required="true()" />
			<bind nodeset="/data/birthPlaceOther" type="string" required="true()"
				relevant="/data/birthPlace='5'" />
			<bind nodeset="/data/umbilicalCordCutting" type="select1"
				required="true()" />
