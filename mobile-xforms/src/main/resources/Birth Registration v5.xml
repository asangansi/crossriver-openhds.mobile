			<bind nodeset="/data/visitId" type="string"
				required="true()" constraint="regex(., '^[A-Za-z]{10}[0-9]{5}$')"
				jr:constraintMsg="Visit Id is not the right format" />
			<bind nodeset="/data/visitDate" type="date" constraint=". &lt;= today()"
